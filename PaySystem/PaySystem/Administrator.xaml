<Window x:Class="PaySystem.Administrator"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PaySystem"
        mc:Ignorable="d"
        Title="Administrator" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="True">
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="File">
                    <MenuItem Header="OperatorList" Click="MenuItem_Click">
                        
                    </MenuItem>
                    <Separator></Separator>
                    <MenuItem Header="Add Operator">
                        
                    </MenuItem>
                    <Separator></Separator>
                </MenuItem>
            </Menu>
        </DockPanel>

        <StackPanel>
            <Frame x:Name="frameMain"/>
        </StackPanel>
    </Grid>
</Window>
